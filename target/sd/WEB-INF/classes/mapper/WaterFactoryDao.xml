<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imudges.dao.WaterFactoryDao">
    <select id="queryAllWaterFactory" resultType="WaterFactory">
        SELECT *FROM waterpipe.waterfactory;
    </select>
    <select id="queryById" resultType="WaterFactory">
        SELECT *FROM waterpipe.waterfactory
        WHERE wf_id=#{wfId};
    </select>
</mapper>
